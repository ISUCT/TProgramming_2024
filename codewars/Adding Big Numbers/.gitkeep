function add(a, b) {
    a = a.split('').reverse()
    b = b.split('').reverse()
    let c = 0
    res = []
    maxLen = Math.max(a.length, b.length)
    for (let i = 0; i < maxLen; i++){
        let num1 = (i < a.length) ? parseInt(a[i]) : 0
        let num2 = (i < b.length) ? parseInt(b[i]) : 0
        let curr = num1 + num2 + c
        c = Math.floor(curr / 10)
        console.log(num1, num2, c)
        res.push(curr % 10)
        if (maxLen - i === 1 && c !== 0) {
            res.push(c)
        }
    }
    res.reverse()
    console.log(res.join(''))
}